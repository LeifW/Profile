Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 100
       , commands = [ Run Weather "KPDX" ["-t"," <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Wireless "wlo1" [] 10
                    , Run BatteryP ["BAT0"] ["-t", "AC: <acstatus> <left>%", "-L","50","-H","75","--high","green","--normal","yellow", "--low", "red"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , lowerOnStart = True
       , alignSep = "}{"
       , template = "%StdinReader% }{ %wlo1wi% | %battery% | %cpu% | %memory% * %swap%   <fc=#ee9a00>%date%</fc> | %KPDX%"
       }
